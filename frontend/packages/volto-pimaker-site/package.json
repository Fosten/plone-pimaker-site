{
  "name": "volto-pimaker-site",
  "version": "2.0.0-alpha.0",
  "description": "Plone 6 website for PiMaker",
  "main": "src/index.js",
  "license": "MIT",
  "keywords": [
    "volto-addon",
    "volto",
    "plone",
    "react"
  ],
  "author": "Brian Davis",
  "homepage": "https://github.com/fosten/volto-pimaker-site#readme",
  "repository": {
    "type": "git",
    "url": "git@github.com:fosten/volto-pimaker-site.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "i18n": "rm -rf build/messages && NODE_ENV=production i18n --addon",
    "dry-release": "release-it --dry-run",
    "release": "release-it",
    "release-major-alpha": "release-it major --preRelease=alpha",
    "release-alpha": "release-it --preRelease=alpha"
  },
  "addons": [
    "@eeacms/volto-accordion-block",
    "@eeacms/volto-banner",
    "@eeacms/volto-columns-block",
    "@eeacms/volto-matomo",
    "@kitconcept/volto-slider-block",
    "volto-form-block",
    "@plone-collective/volto-rss-provider"
  ],
  "dependencies": {
    "@eeacms/volto-accordion-block": "*",
    "@eeacms/volto-banner": "*",
    "@eeacms/volto-columns-block": "*",
    "@eeacms/volto-matomo": "*",
    "@kitconcept/volto-slider-block": "*",
    "volto-form-block": "*",
    "@plone-collective/volto-rss-provider": "*",
    "querystring-es3": "*"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@plone/scripts": "^3.6.1",
    "release-it": "^17.1.1"
  }
}
