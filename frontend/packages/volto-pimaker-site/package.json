{
  "name": "volto-pimaker-site",
  "version": "2.0.0-alpha.0",
  "description": "Plone 6 website for PiMaker",
  "main": "src/index.js",
  "license": "MIT",
  "keywords": [
    "volto-addon",
    "volto",
    "plone",
    "react"
  ],
  "author": "Brian Davis",
  "homepage": "https://github.com/fosten/volto-pimaker-site#readme",
  "repository": {
    "type": "git",
    "url": "git@github.com:fosten/volto-pimaker-site.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "i18n": "rm -rf build/messages && NODE_ENV=production i18n --addon",
    "dry-release": "release-it --dry-run",
    "release": "release-it",
    "release-major-alpha": "release-it major --preRelease=alpha",
    "release-alpha": "release-it --preRelease=alpha"
  },
  "addons": [
    "@eeacms/volto-accordion-block",
    "@eeacms/volto-banner",
    "@eeacms/volto-columns-block",
    "@eeacms/volto-matomo",
    "@kitconcept/volto-slider-block",
    "volto-form-block",
    "@plone-collective/volto-rss-provider",
    "@plone-collective/volto-authomatic"
  ],
  "dependencies": {
    "@eeacms/volto-accordion-block": "*",
    "@eeacms/volto-banner": "*",
    "@eeacms/volto-columns-block": "*",
    "@eeacms/volto-matomo": "*",
    "@kitconcept/volto-slider-block": "*",
    "volto-form-block": "*",
    "@plone-collective/volto-rss-provider": "*",
    "querystring-es3": "*",
    "@plone-collective/volto-authomatic": "3.0.0-alpha.1"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@plone/registry": "workspace:*",
    "@plone/scripts": "workspace:*",
    "@plone/types": "workspace:*",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "typescript": "^5.7.3",
    "@testing-library/react": "^16.2.0",
    "release-it": "^19.0.5",
    "vitest": "^3.1.2"
  }
}
